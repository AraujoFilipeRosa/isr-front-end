{
  "version": 2,
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "devCommand": "yarn dev",
  "buildCommand": "yarn build",
  "outputDirectory": "dist",
  "framework": "vite"
}
